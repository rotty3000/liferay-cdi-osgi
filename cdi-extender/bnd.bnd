Export-Package: com.liferay.cdi.provider;cdi-api-bundle=true;mandatory:=cdi-api-bundle
Provide-Capability: \
	osgi.extender; \
		osgi.extender='osgi.cdi'; \
		uses:="org.osgi.service.cdi,javax.enterprise.inject.spi"; \
		version:Version="${Bundle-Version}"